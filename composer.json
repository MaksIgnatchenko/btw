{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "braintree/braintree_php": "^3.26",
        "cviebrock/image-validator": "^2.2",
        "davejamesmiller/laravel-breadcrumbs": "^4.2",
        "doctrine/dbal": "^2.6",
        "edujugon/push-notification": "3.0.*",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "~6.0",
        "infyomlabs/adminlte-templates": "5.5.x-dev",
        "infyomlabs/laravel-generator": "5.5.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "intervention/image": "^2.4",
        "jlapp/swaggervel": "dev-master",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5.0",
        "marcl/amazonproductapi": "^3.0",
        "nesbot/carbon": "^1.22",
        "ramsey/uuid": "^3.7",
        "santigarcor/laratrust": "5.0.*",
        "sentry/sentry-laravel": "^0.9.0",
        "tymon/jwt-auth": "1.0.0-rc.1",
        "yajra/laravel-datatables-buttons": "^3.0",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
